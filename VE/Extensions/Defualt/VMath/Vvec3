#pragma once 
typedef struct Vvec3 
{
	float x, y, z;
	Vvec3()
	{
		this->x = 0;
		this->y = 0;
		this->z = 0;
	}
	Vvec3(float var)
	{
		this->x = var;
		this->y = var;
		this->z = var;
	}
	Vvec3(float x, float y, float z)
	{
		this->x = x;
		this->y = y;
		this->z = z;
	}
}Vvec3, Vvector3, Vfloat3;